{
  "name": "@copper/plugin-cognito",
  "version": "1.0.2",
  "description": "Architect (arc.codes) serverless framework plugin that defines a Cognito User Pool and associated Lambdas triggers",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --fix --ignore-pattern \"sample-app\"",
    "test:unit": "jasmine --config=test/jasmine.json",
    "test": "npm run lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copperinc/plugin-cognito.git"
  },
  "keywords": [
    "serverless",
    "arc",
    "cloudformation",
    "cognito",
    "user",
    "pool",
    "lambda"
  ],
  "author": "Copper Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/copperinc/plugin-cognito/issues"
  },
  "homepage": "https://github.com/copperinc/plugin-cognito#readme",
  "dependencies": {
    "@architect/utils": "^2.0.5",
    "param-case": "^3.0.4"
  },
  "devDependencies": {
    "@architect/eslint-config": "^1.0.0",
    "@architect/inventory": "^1.3.3-RC.0",
    "@architect/package": "^6.2.1-RC.0",
    "eslint": "^7.26.0",
    "fs-extra": "^10.0.0",
    "jasmine": "^3.7.0"
  },
  "peerDependencies": {
    "@architect/architect": "^8.6.0-RC.0"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config",
    "rules": {
      "brace-style": "off",
      "indent": [
        "error",
        4
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
